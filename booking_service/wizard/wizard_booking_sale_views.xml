<odoo>
     <record id="confirm_ignore_warning" model="ir.ui.view">
        <field name="name">booking_service.confirm_ignore_warning</field>
        <field name="model">wizard.booking.sale</field>
        <field name="arch" type="xml">
            <form>
	            <field name="sale_id" invisible="1"/>
	            <field name="picking_id" invisible="1"/>
	            <field name="note" readonly="1"/>
                <footer>
                    <button name="button_confirm" type="object" string="Confirm" class="oe_highlight"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
